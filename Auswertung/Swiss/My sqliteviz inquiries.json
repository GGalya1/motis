            "query": "WITH 'tso_cpu' (search_type, cpu_avg) as (SELECT search_type, val as 'cpu_avg' FROM \"analysis_swiss\" where search_type like 'tso%' and target like 'raptor_cpu' and stat_name like 'avg_total_calc%'),\n     'tso_gpu' (search_type, gpu_avg) as (SELECT search_type, val as 'gpu_avg' FROM \"analysis_swiss\" where search_type like 'tso%' and target like 'raptor_gpu' and stat_name like 'avg_total_calc%'),\n     'routing' (search_type, threshold) as (with 'search_types' (search_type) as (select search_type from \"analysis_swiss\" WHERE target like '%gpu' and search_type like 'tso%' and stat_name like 'avg_total_calc%')\n\t\t\t\t\t\t\t\t\t\t\tSELECT search_types.search_type, val as 'threshold' FROM \"analysis_swiss\", \"search_types\" where analysis_swiss.search_type like 'time%' and target like 'routing' and stat_name like 'avg_total_calc%')\nSELECT cpu.search_type, substr(cpu.search_type, -2, 2) as 'slot_size', cpu.cpu_avg, gpu.gpu_avg, routing.threshold from \"tso_cpu\" as 'cpu', \"tso_gpu\" as 'gpu', \"routing\" where gpu.search_type like cpu.search_type and routing.search_type like cpu.search_type",
